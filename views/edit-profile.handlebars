<h1> Edit Profile </h1>


<form class="register-form">
        <div class="form-container">
          <h1>Edit Profile </h1>
          <p>Please fill in this form to create an account.</p>
          <b>First name</b>
          <input type="text" placeholder="Enter first name" name="fname" id="first_name">
          <button type ="submit" id="edit-firstbtn">Edit</button>
         <b>Last name</b>
          <input type="text" placeholder="Enter last name" name="lname" id="last_name">
          <button type ="submit" id="edit-lastbtn">Edit</button>

          <b>Username</b>
          <input type="text" placeholder="Enter Username" name="user" id="username">
          <button type ="submit" id="edit-userbtn">Edit</button>
      
          <b>Email</b>
          <input type="text" placeholder="Enter Email" name="mail" id="email">
          <button type ="submit" id="edit-emailbtn">Edit</button>
          
          
      

        </div>
        
        <div class="container signin">
        </div>
      </form>
       <script src="/scripts/edit-profile.js"></script>